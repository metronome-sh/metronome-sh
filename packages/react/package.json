{
  "name": "@metronome-sh/react",
  "version": "5.0.3",
  "description": "Metronome - Insights for Remix",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build:babel": "babel src --out-dir dist --extensions .ts,.js,.tsx",
    "build:typings": "tsc --pretty --declaration --emitDeclarationOnly --outDir dist/",
    "build": "concurrently -s all \"npm:build:*\"",
    "dev:babel": "babel src --out-dir dist --extensions .ts,.js,.tsx --watch",
    "dev:typings": "tsc -w --pretty --declaration --emitDeclarationOnly --outDir dist/",
    "dev": "concurrently \"npm:dev:*\""
  },
  "keywords": [
    "metronome",
    "remix",
    "analytics",
    "insights",
    "performance"
  ],
  "author": "Erick Tamayo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/metronome-sh/metronome-sh/issues"
  },
  "homepage": "https://github.com/metronome-sh/metronome-sh#readme",
  "dependencies": {
    "concurrently": "^7.1.0",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@metronome-sh/config": "*",
    "@remix-run/react": ">=1.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^17.0.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^18.0.1",
    "babel-plugin-search-and-replace": "^1.1.0",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@remix-run/react": ">=1.3.0",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  }
}
