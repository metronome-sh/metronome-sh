{
  "name": "@metronome-sh/cloudflare-pages",
  "version": "5.0.2",
  "description": "Metronome - Insights for Remix",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:typings": "tsc --declaration --emitDeclarationOnly -p ./tsconfig.json --outDir dist/",
    "build:rollup": "rollup -c",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:typings": "tsc --declaration --emitDeclarationOnly -p ./tsconfig.json --outDir dist/ --watch",
    "dev:rollup": "rollup -c --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "metronome",
    "remix",
    "analytics",
    "insights",
    "performance",
    "cloudflare"
  ],
  "author": "Erick Tamayo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/metronome-sh/metronome-sh/issues"
  },
  "homepage": "https://github.com/metronome-sh/metronome-sh#readme",
  "dependencies": {
    "@metronome-sh/runtime": "5.0.2",
    "concurrently": "^7.1.0",
    "ts-invariant": "^0.9.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.6.0",
    "@metronome-sh/config": "*",
    "@types/node": "^17.0.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@remix-run/architect": ">=1.4.1",
    "@remix-run/dev": ">=1.4.1",
    "@remix-run/node": ">=1.4.1"
  }
}
