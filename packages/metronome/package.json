{
  "name": "metronome-sh",
  "version": "8.0.1",
  "description": "Metronome - Insights for Remix",
  "module": "dist/esm/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "bin": {
    "metronome": "./cli.js"
  },
  "scripts": {
    "build:esbuild": "node --experimental-import-meta-resolve esbuild.mjs",
    "build:typings": "tsc --pretty --declaration --emitDeclarationOnly --outDir dist/",
    "build": "concurrently -s all \"npm:build:*\"",
    "dev:esbuild": "node --experimental-import-meta-resolve esbuild.watch.mjs",
    "dev:typings": "tsc -w --pretty --declaration --emitDeclarationOnly --outDir dist/",
    "dev": "concurrently \"npm:dev:*\""
  },
  "keywords": [
    "metronome",
    "remix",
    "analytics",
    "insights",
    "performance",
    "express"
  ],
  "author": "Erick Tamayo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/metronome-sh/metronome-sh/issues"
  },
  "homepage": "https://github.com/metronome-sh/metronome-sh#readme",
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/resources": "^1.19.0",
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "@opentelemetry/sdk-trace-web": "^1.19.0",
    "@opentelemetry/semantic-conventions": "^1.19.0",
    "is-ip": "^5.0.1",
    "magic-string": "^0.30.5",
    "superjson": "^2.2.1",
    "ts-invariant": "^0.9.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^7.1.0",
    "esbuild": "^0.17.10",
    "esbuild-plugin-replace": "^1.3.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "@remix-run/react": "^2.4.0",
    "@remix-run/server-runtime": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    "./react": {
      "import": "./dist/esm/react.js",
      "types": "./dist/react/react.d.ts"
    },
    "./vite": {
      "import": "./dist/esm/vite.js",
      "types": "./dist/vite/vite.d.ts"
    },
    "./server": {
      "import": "./dist/esm/server.js",
      "types": "./dist/server/server.d.ts"
    }
  }
}
