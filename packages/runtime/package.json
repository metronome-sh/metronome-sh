{
  "name": "@metronome-sh/runtime",
  "version": "5.3.7",
  "description": "Metronome - Insights for Remix",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:typings": "tsc --declaration --emitDeclarationOnly -p ./tsconfig.json --outDir dist/",
    "build:rollup": "wait-on dist/script/index.js && rollup -c",
    "build:script": "rollup -c rollup.config.script.js",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:typings": "tsc --declaration --emitDeclarationOnly -p ./tsconfig.json --outDir dist/ --watch",
    "dev:rollup": "wait-on dist/script/index.js && rollup -c --watch",
    "dev:script": "rollup -c rollup.config.script.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "metronome",
    "remix",
    "analytics",
    "insights",
    "performance"
  ],
  "author": "Erick Tamayo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/metronome-sh/metronome-sh/issues"
  },
  "homepage": "https://github.com/metronome-sh/metronome-sh#readme",
  "dependencies": {
    "@remix-run/server-runtime": ">=1.3.0",
    "@remix-run/react": ">=1.3.0",
    "concurrently": "^7.1.0",
    "dayjs": "^1.11.1",
    "nanoid": "^3.3.3",
    "superstruct": "^0.15.4",
    "ts-invariant": "^0.9.4",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@metronome-sh/config": "5.3.7",
    "@remix-run/dev": "^1.2.3",
    "@types/node": "^17.0.1",
    "typescript": "^4.5.4"
  }
}
